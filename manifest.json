{
  "manifest_version": 2,

  "name": "AlphaGriffinDice",
  "description": "Play dice the Alphagriffin way.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "img/logo.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "tabs"
  ],
  "content_scripts" : [{
   "matches" : [ "https://yobit.net/en/dice/*"],
   "js" : ["js/hijacked.js"]
  }]
}
